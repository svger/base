{
  "name": "cefc-utils",
  "version": "0.0.11",
  "description": "utils",
  "main": "lib/cefc-utils.js",
  "module": "es/cefc-utils.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir ./lib",
    "build:es": "cross-env BABEL_ENV=es babel ./src --out-dir ./es",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/cefc-utils.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/cefc-utils.min.js",
    "build": "npm run clean && npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "prepare": "npm run clean && npm run build",
    "clean": "rimraf dist es lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@10.199.101.162:CEFC-UTILS/utils.git"
  },
  "keywords": [
    "utils"
  ],
  "author": "Kane",
  "license": "ISC",
  "dependencies": {
    "md5": "~2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "cefc-tools": "^0.0.15",
    "cross-env": "^5.1.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "symbol-observable": "^1.1.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "Android >= 4",
              "iOS >= 7",
              "Safari >= 7",
              "not ie <= 8"
            ]
          },
          "modules": false,
          "loose": true
        }
      ],
      "stage-1"
    ],
    "env": {
      "commonjs": {
        "presets": [
          [
            "env",
            {
              "loose": true
            }
          ]
        ]
      }
    }
  },
  "npmName": "cefc-utils",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
